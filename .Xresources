URxvt.font: \
            xft:NotoSansM Nerd Font Mono:size=10, \
            xft:Noto Sans Mono:style=Normal:antialias=true:size=10:minspace=False, \
            xft:UbuntuMono Nerd Font Mono:style=Regular:antialias=true:size=10:minspace=False, \
            xft:SauceCodePro Nerd Font:style=Regular:antialias=true:size=10:minspace=False, \
            xft:DejaVuSansMono Nerd Font:antialias=true:size=10minspace=False, \
            xft:DroidSansMono Nerd Font Mono:antialias=true:size=10, \
            xft:FuraMono NF:size=16:antialias=true:minspace=False, \
            xft:RobotoMono Nerd Font:antialias=true:size=10:minspace=False, \
            xft:Noto Sans Mono:antialias=true:size=10:minspace=False, \
            xft:Monospace:style=Medium:antialias=true:minspace=False

URxvt.keysym.C-minus:  resize-font:smaller
URxvt.keysym.C-plus:   resize-font:bigger
URxvt.keysym.C-equal:  resize-font:equal

URxvt.url-launcher: chromium
URxvt.matcher.button: 1
URxvt.keysym.M-u: perl:matcher:select
URxvt.scrollstyle: plain
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.perl-ext-common: url-select,keyboard-select,resize-font,matcher,selection-to-clipboard,readline

